  <script type="text/javascript">
    function limpiarCajas() {
      $(":text").each(function() {
        $($(this)).val('');
      });
      $('input[name="password"]').each(function() {
        $($(this)).val('');
      });
      $('#alerta').each(function() {
        $($(this)).val('');
      });
    }
  </script>
