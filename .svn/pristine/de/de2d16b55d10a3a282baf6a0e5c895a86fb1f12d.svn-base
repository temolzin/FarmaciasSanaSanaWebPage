      $(document).ready(pagination(1));  

      function pagination(partida){
        var url = 'funciones/procesos/paginador.php';
        $.ajax({
          type:'POST',
          url: url,
          data:'partida='+partida,
          success:function(data) {
            var array = eval(data);
            $('#paginarImagenes').html(array[0]);
            $('#pagination').html(array[1]);
            //$('#linkEstilos').attr("href", "asset/css/bootstrap.min.css");
            //Codigo de prueba
          }
        });
        return false;
      }
    </script>

